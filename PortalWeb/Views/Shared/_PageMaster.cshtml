<!DOCTYPE html>
<html ng-app="newApp">
<head>
    <meta charset="utf-8" />
    <title> REGISTRO DE USUARIO </title>

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Mystyle.css" rel="stylesheet" />

    @{
        ViewBag.Title = "_PageMaster";
    }
</head>
<!--<body ng-controller="">-->
<body>

    <div class="wrapper">
        <header class="header header-fixed navbar container-fluid">
            <div ng-controller="UserController" class="row" style="text-align: left">
                <div class="brand col-sm-3"></div>
                <a href="#" class="navbar-brand">
                    <i class="glyphicon glyphicon-bitcoin"></i>
                    <span class="glyphicon-font"> ITECNOLOGY- SOLARIS</span>
                </a>
                <a href="/User/CerrarSession" class="navbar-brand" style="float: right">
                    <i class="glyphicon glyphicon-bitcoin"></i>
                    <span class="glyphicon-font">Ha Iniciado Session Como {{UserLogin}} : Salir </span>
                </a>
                @*@Html.ActionLink("Salir", "CerrarSession", "User", new { @class = "navbar-brand", style = "float: right" })*@
            </div>
        </header>
        <div class="box">
            <div class="row">
                <!--inicio sidebar-->
                <div class="column col-sm-3" id="sidebar">
                    <p class="nav-title">
                        Opciones
                        <a href="#" class="pull-right">
                            <i class="glyphicon glyphicon-apple"></i>
                        </a>
                    </p>
                    <ul class="nav">
                        <li>
                            <a href="#"><i class="glyphicon glyphicon-tags"></i> Subir archivo </a>
                        </li>

                        <li>
                            <a href="#"><i class="glyphicon glyphicon-tags"></i> Calendario </a>
                        </li>
                    </ul>
                </div>
                <!--fin sidebar-->
                <!--inicio cuerpo-->
                <div id="main" class="column col-sm-9">
                    @RenderBody()

                </div>
                <!--fin del cuerpo-->

            </div>
        </div>
    </div>

    <!--inicio de formulario modal registro usuario-->




    <footer>
        <p>&copy; @DateTime.Now.Year - Mi aplicación ASP.NET</p>
    </footer>



    <!--fin de modal registro usuario-->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js"></script>
    <script src="~/Scripts/MyScripts/services/UserService.js"></script>
    <script src="~/Scripts/MyScripts/controllers/UserController.js"></script>
    <script src="~/Scripts/MyScripts/newApp.js"></script>
    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
</body>
</html>
